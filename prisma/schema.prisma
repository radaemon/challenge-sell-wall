generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Collection {
  slug     String    @id @unique
  listings Listing[]
}

model Listing {
  tokenId    String     @id
  listPrice  Float
  collection Collection @relation(fields: [tokenId], references: [slug])
}
