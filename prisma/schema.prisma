generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Collection {
  address  String    @id @unique
  name     String
  listings Listing[]
}

model Listing {
  tokenId           Int         @id
  collectionAddress String
  Collection        Collection? @relation(fields: [collectionAddress], references: [address])
}
